{
  "name": "randomwordsgenerator",
  "version": "1.1.0",
  "description": "Mini library to generate random english words from Dictionary",
  "main": "RWG.js",
  "types": "RWG.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node --loader ts-node/esm .\\src\\index.ts",
    "publish": "npx tsc",
    "postpublish": "npm run copyAssets && npm run copyInfo",
    "copyAssets": "copyfiles --error -u 2 src/lib/words.zip dist",
    "copyInfo" : "copyfiles --error package.json readme.md dist"
  },
  "keywords": [
    "random",
    "word",
    "generator",
    "english"
  ],
  "author": "LayKhatri",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laykhatri/randomwordsgenerator.git"
  }
}
